<p id="notice"><%= notice %></p>

<h1>Share Configs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Desc</th>
      <th>Img url</th>
      <th>Link url</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @share_configs.each do |share_config| %>
      <tr>
        <td><%= share_config.title %></td>
        <td><%= share_config.desc %></td>
        <td><%= share_config.img_url %></td>
        <td><%= share_config.link_url %></td>
        <td><%= share_config.user_id %></td>
        <td><%= link_to 'Show', share_config %></td>
        <td><%= link_to 'Edit', edit_share_config_path(share_config) %></td>
        <td><%= link_to 'Destroy', share_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Share Config', new_share_config_path %>

<%- model_class = Message -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:psz_sub_port) %>:</strong></dt>
  <dd><%= @message.psz_sub_port %></dd>
  <dt><strong><%= model_class.human_attribute_name(:psz_msg) %>:</strong></dt>
  <dd><%= @message.psz_msg %></dd>
  <dt><strong><%= model_class.human_attribute_name(:psz_mobis) %>:</strong></dt>
  <dd><%= @message.psz_mobis %></dd>
  <dt><strong><%= model_class.human_attribute_name(:request_ip) %>:</strong></dt>
  <dd><%= @message.request_ip %></dd>
  <dt><strong><%= model_class.human_attribute_name(:channel_name) %>:</strong></dt>
  <dd><%= @message.channel_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:response_code) %>:</strong></dt>
  <dd><%= @message.response_code %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              messages_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_message_path(@message), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              message_path(@message),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>

<!-- 钻币礼包删除modal -->
<div class="modal fade" id="deleteCoinbagModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title" id="exampleModalLabel">钻币礼包删除</h4>
      </div>
      <form action="/admins/coin_bags/destroy_coin" method="post">
        <div class="modal-body">
          <h3>确定删除钻币礼包?</h3>

          <input type="hidden"  class="form-control" name='id' id="del_coin_id">

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
          <button type="submit" class="btn btn-primary">确定</button>
        </div>
      </form>
    </div>
  </div>
</div>